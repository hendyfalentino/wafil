        android:text="Jenis Barang"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvlb5"
        app:layout_constraintEnd_toEndOf="@+id/tvlb5"
        app:layout_constraintStart_toStartOf="@+id/tvlb5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Tanggal"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvlb6"
        app:layout_constraintEnd_toEndOf="@+id/tvlb6"
        app:layout_constraintStart_toStartOf="@+id/tvlb6" />
</LinearLayout>